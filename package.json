{
  "name": "browser-cjs",
  "version": "1.0.1",
  "description": "A minimal CommonJS-compatible module loader for the browser environment.",
  "main": "require.js",
  "scripts": {
    "build": "babel --presets @babel/env --watch require.es6 --out-file require.js",
    "minify": "uglifyjs require.js -o require.min.js"
  },
  "keywords": [
    "require",
    "js",
    "browser",
    "commonjs",
    "cjs",
    "module loader",
    "nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/luciVuc/browser-cjs"
  },
  "homepage": "https://github.com/luciVuc/browser-cjs#readme.md",
  "author": "Lucian Vuc (https://github.com/luciVuc)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "uglify-js": "^3.6.0"
  },
  "dependencies": {}
}
